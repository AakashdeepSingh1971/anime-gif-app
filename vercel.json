{
    "version": 2,
    "builds": [
        {
            "src": "api/random-gif.js",
            "use": "@vercel/node"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/random-gif",
            "dest": "/api/random-gif.js"
        },
        {
            "src": "/",
            "dest": "index.html"
        }
    ]
}